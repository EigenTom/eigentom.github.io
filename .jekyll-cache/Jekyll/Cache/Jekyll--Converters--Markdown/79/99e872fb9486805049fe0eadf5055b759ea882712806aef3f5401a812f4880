  <span class="nl">cursor</span><span class="p">:</span> <span class="nb">pointer</span><span class="p">;</span>
<span class="p">}</span>

<span class="nc">.toolbox</span> <span class="o">&gt;</span> <span class="o">*</span><span class="nd">:last-child</span> <span class="p">{</span>
  <span class="nl">margin-left</span><span class="p">:</span> <span class="nb">auto</span><span class="p">;</span>
